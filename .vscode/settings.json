  "psi-header.config": {
    "forceToTop": true,
    "blankLinesAfter": 1,
    "license": "Custom"
  },
  "psi-header.changes-tracking": {
    "isActive": true,
    "include": [
      "cpp",
      "c",
      "shellscript",
      "python",
    ],
    "exclude": [
      "markdown",
      "json",
      "plaintext"
    ],
    "excludeGlob": [
      "out/**",
      "src/**/*.xyz",
      "build/**"
    ],
    "autoHeader": "manualSave"
  },
  "psi-header.license-text": [
    "Licensed under the MIT license."
  ],
  "psi-header.variables": [
    [
      "company",
      "Microsoft Corporation."
    ],
    [
      "author",
      "Weihao Cui"
    ],
    [
      "authoremail",
      "v-weihaocui@microsoft.com"
    ]
  ],
  "psi-header.lang-config": [
    {
      "language": "python",
      "begin": "",
      "prefix": "# ",
      "end": "",
      "blankLinesAfter": 1,
    },
    {
      "language": "shellscript",
      "begin": "",
      "prefix": "# ",
      "end": "",
      "blankLinesAfter": 1,
      "beforeHeader": [
        "#!/usr/bin/env bash"
      ]
    },
    {
      "language": "*",
      "begin": "/*!"
    }
  ],
  "psi-header.templates": [
    {
      "language": "*",
      "template": [
        "Copyright (c) <<year>> by <<company>>",
        "<<licensetext>>",
      ]
    }
  ],
  "python.testing.unittestArgs": [
    "-v",
    "-s",
    "./test/python",
    "-p",
    "*_test.py"
  ],
  "python.testing.pytestEnabled": false,
  "python.testing.unittestEnabled": true,
  "git.detectSubmodules": false,
  "C/C++ Include Guard.Auto Update Path Allowlist": [
    "include",
    "src"
  ],
  "C/C++ Include Guard.Path Skip": 1,
  "C/C++ Include Guard.Subfolder Prefixes": [
    {
      "folderPath": "include",
      "prefix": ""
    },
    {
      "folderPath": "src",
      "prefix": "BRT_"
    }
  ],
}