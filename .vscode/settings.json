{
  "python.envFile": "${workspaceFolder}/.env",
  "files.associations": {
    "*.pbtxt": "textproto",
    "__hash_table": "cpp",
    "__split_buffer": "cpp",
    "__tree": "cpp",
    "array": "cpp",
    "bitset": "cpp",
    "deque": "cpp",
    "initializer_list": "cpp",
    "list": "cpp",
    "map": "cpp",
    "queue": "cpp",
    "random": "cpp",
    "regex": "cpp",
    "set": "cpp",
    "stack": "cpp",
    "string": "cpp",
    "string_view": "cpp",
    "unordered_map": "cpp",
    "unordered_set": "cpp",
    "valarray": "cpp",
    "vector": "cpp",
    "*.inc": "cpp",
    "*.ipp": "cpp",
    "ranges": "cpp",
    "utility": "cpp",
    "iterator": "cpp"
  },
  "psi-header.config": {
    "forceToTop": true,
    "blankLinesAfter": 1,
    "license": "Custom"
  },
  "psi-header.changes-tracking": {
    "isActive": true,
    "modAuthor": "Modified By: ",
    "modDate": "Last Modified: ",
    "modDateFormat": "dddd, MMMM Do YYYY, h:mm:ss a",
    "include": [
      "cpp",
      "c",
      "shellscript",
      "python",
    ],
    "exclude": [
      "markdown",
      "json",
      "plaintext"
    ],
    "excludeGlob": [
      "out/**",
      "src/**/*.xyz",
      "build/**"
    ],
    "autoHeader": "manualSave"
  },
  "psi-header.license-text": [
    "Licensed under the MIT license."
  ],
  "psi-header.variables": [
    [
      "company",
      "Microsoft Corporation."
    ],
    [
      "author",
      "raphael hao"
    ],
    [
      "authoremail",
      "raphaelhao@outlook.com"
    ]
  ],
  "psi-header.lang-config": [
    {
      "language": "python",
      "begin": "",
      "prefix": "# ",
      "end": "",
      "blankLinesAfter": 1,
    },
    {
      "language": "shellscript",
      "begin": "",
      "prefix": "# ",
      "end": "",
      "blankLinesAfter": 1,
      "beforeHeader": [
        "#!/usr/bin/env bash"
      ]
    },
    {
      "language": "*",
      "begin": "/*!"
    }
  ],
  "psi-header.templates": [
    {
      "language": "*",
      "template": [
        "Copyright (c) <<year>> by <<company>>",
        "<<licensetext>>",
        // "--------",
        // "\\file: <<filerelativepath>>",
        // "\\brief: ",
        // "Created Date: <<filecreated('dddd, MMMM Do YYYY, h:mm:ss a')>>",
        // "Author: <<author>>"
        // "Email: <<authoremail>>",
        // "--------",
        // "Last Modified: ",
        // "Modified By: "
      ]
    }
  ],
  "python.testing.unittestArgs": [
    "-v",
    "-s",
    "./test/unittest",
    "-p",
    "*_test.py"
  ],
  "python.testing.pytestEnabled": false,
  "python.testing.unittestEnabled": true,
}